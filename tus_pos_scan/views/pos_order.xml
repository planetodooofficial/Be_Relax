<odoo>
    <!-- Inherit Form View to Modify it -->
    <record id="view_pos_pos_form_tus" model="ir.ui.view">
        <field name="name">pos.order.form.tus</field>
        <field name="model">pos.order</field>
        <field name="inherit_id" ref="point_of_sale.view_pos_pos_form"/>
        <field name="arch" type="xml">

            <xpath expr="//notebook" position="inside">
                <page name="boarding_pass" string="Boarding Pass">
                    <field name="boarding_pass_ids"/>
                </page>
            </xpath>

        </field>
    </record>
</odoo>
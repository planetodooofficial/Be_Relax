<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="SelectionPopup" t-inherit="point_of_sale.SelectionPopup" owl="1">
        <xpath expr="//header" position="after">
            <t t-if="env.pos.cashier.id">
                <div class="search-bar-container sb-partner">
                    <div class="pos-search-bar" style="width: 50%;text-align:end;">
                        <input t-ref="search-word-input-partner" placeholder="Search Therapist..." size="1"
                               t-on-keyup="updateEmployeeList" style="border-radius:30px;margin: 5px;height: 40px;"/>
                    </div>
                    <i class="fa fa-search" t-on-click="_onPressEnterKey" style="margin: 0px 25px;"/>
                    </div>
            </t>
        </xpath>
    </t>
</templates>